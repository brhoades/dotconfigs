#
# Executes commands at the start of an interactive session.
#
# Authors:
#   Sorin Ionescu <sorin.ionescu@gmail.com>
# Source Prezto.
if [[ -s "${ZDOTDIR:-$HOME}/.zprezto/init.zsh" ]]; then
  source "${ZDOTDIR:-$HOME}/.zprezto/init.zsh"
fi
setopt clobber
setopt no_rm_star_silent

# Customize to your needs...

export TERM='xterm-256color'

EDITOR=vim
GIT_EDITOR=$EDITOR
export EDITOR
export GIT_EDITOR

export JAVA_HOME="/usr/lib/jvm/java-8-openjdk"
export ANDROID_SDK_HOME="/opt/android-sdk/"
export PANEL_FIFO="/tmp/panel_fifo"
export PATH="/usr/sbin:$PATH"


if [ -e $HOME/.rbenv/ ]; then
  export PATH="$HOME/.rbenv/bin:$PATH"
  eval "$(rbenv init -)"
fi

if [ -e $HOME/.config/bspwm ]; then
  export PATH="$HOME/.config/bspwm/panel/:$PATH"
fi

if [ -e $HOME/.gem/ruby/2.6.2/bin ]; then
  export PATH=$HOME/.gem/ruby/2.6.2/bin:$PATH
fi

if [ -e $home/.rvm ]; then
  # Add RVM to PATH for scripting. Make sure this is the last PATH variable change.
    export PATH="$PATH:$HOME/.rvm/bin"
fi

if [ -e $HOME/.local/bin ]; then
  export PATH=$HOME/.local/bin:$PATH
fi

bindkey "^[[1;5C" forward-word
bindkey "^[[1;5D" backward-word

alias suspend='sudo systemctl suspend'

setopt clobber

# Virtsh/libvirt
export LIBVIRT_DEFAULT_URI="qemu:///system"

alias rm='rm'
alias dmesg='dmesg --color=always'
alias less='less -r'
bindkey -e

typeset -A ZSH_HIGHLIGHT_STYLES
ZSH_HIGHLIGHT_STYLES[cursor]=underline

export GTEST_INCLUDE_DIRS=/usr/lib/
export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"  # This loads nvm
[ -s "$NVM_DIR/bash_completion" ] && \. "$NVM_DIR/bash_completion"  # This loads nvm bash_completion

export GPG_TTY=$(tty)

