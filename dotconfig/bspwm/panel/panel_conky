short_units yes
background yes
out_to_console yes
out_to_x no
# Update interval in seconds
update_interval 3
update_interval_on_battery 5

TEXT

S${execi 1 ./panel_cpuusage} ${cpu cpu0}  \
${execi 1 ./panel_temperature}  \
${execi 1 ./panel_wifi} ${wireless_essid wlp3s0}  \
${execi 1 ./panel_battery}${battery_percent BAT0}  \
${execi 1 ./panel_battery1}${battery_percent BAT1}\
  ${time %-m/%-d/%y  %I:%M %P} %{-u}
